	<step id="95975c47-82fa-42b6-9705-6f52dac5a4cc" location="7 1" name="Reinitialize" factory-id="action-step">
		<start-script>def onStart(chart, step):
	"""
	This will run when the step starts, before any
	other action.

	Arguments:
		chart: A reference to the chart's scope.
		step: A reference to this step's scope.
	"""
	chart.estop=int(system.tag.read("[Scada]ITE/iFMS/ProductionArea/FMSLine/IML/Status/Estop_Sts").value)
	chart.error=int(system.tag.read("[Scada]ITE/iFMS/ProductionArea/FMSLine/IML/Status/InAlarm_Sts").value)
	chart.machineMode=int(system.tag.read("[Scada]ITE/iFMS/ProductionArea/FMSLine/IML/Status/Remote_Sts").value)
#	shared.cells.flagevents("MachineMode","IML",machineMode) #MachineMode
	shared.cells.flagevents("EStop","IML",chart.estop)
	shared.cells.flagevents("Error","IML",chart.error)
	
	if chart.estop==1:
		system.tag.write("[Scada]ITE/iFMS/ProductionArea/FMSLine/IML/Status/Messages","Emergency Stop")
	elif chart.error==1:
		system.tag.write("[Scada]ITE/iFMS/ProductionArea/FMSLine/IML/Status/Messages","Alarm/Error")
	else:
		system.tag.write("[Scada]ITE/iFMS/ProductionArea/FMSLine/IML/Status/Messages","MachineMode Changed")</start-script>
	<timer-script interval="1000">def onTimer(chart, step):
	"""
	This will run at a fixed interval while the
	step is active.

	Arguments:
		chart: A reference to the chart's scope.
		step: A reference to this step's scope.
	"""
	chart.estop=int(system.tag.read("[Scada]ITE/iFMS/ProductionArea/FMSLine/IML/Status/Estop_Sts").value)
	chart.error=int(system.tag.read("[Scada]ITE/iFMS/ProductionArea/FMSLine/IML/Status/InAlarm_Sts").value)
	if chart.estop==1:
		system.tag.write("[Scada]ITE/iFMS/ProductionArea/FMSLine/IML/Status/Messages","Emergency Stop")
	elif chart.error==1:
		system.tag.write("[Scada]ITE/iFMS/ProductionArea/FMSLine/IML/Status/Messages","Alarm Occured")
	else:
		system.tag.write("[Scada]ITE/iFMS/ProductionArea/FMSLine/IML/Status/Messages","MachineMode Changed")</timer-script>
	</step>
	<step id="6ad9c063-ba0e-4c94-ba6a-faa8d8879849" location="5 6" name="RemoteMode" factory-id="action-step">
