	<step id="3b377290-94e9-4ada-8dd4-77c7ed58173d" location="15 2" name="PartUnloaded" factory-id="action-step">
		<start-script>def onStart(chart, step):
	"""
	This will run when the step starts, before any
	other action.

	Arguments:
		chart: A reference to the chart's scope.
		step: A reference to this step's scope.
	"""
	from time import sleep
	shared.cells.flagevents("PartUnloaded","IML",1)
	system.tag.write("[Scada]ITE/iFMS/ProductionArea/FMSLine/IML/Status/Messages","Part Unloaded")
	sleep(1)
	system.tag.write("[Scada]ITE/iFMS/ProductionArea/FMSLine/IML/Command/w_Robot_Finish_s",1)
	system.db.runPrepUpdate("UPDATE master_parts SET processed=1 WHERE PartName=?",[chart.partID],"FMSLine")
	system.tag.write("[Scada]ITE/iFMS/ProductionArea/FMSLine/IML/Command/PartID",0)</start-script>
		<stop-script>def onStop(chart, step):
	"""
	This will run when the step is finished and
	all other actions complete.

	Arguments:
		chart: A reference to the chart's scope.
		step: A reference to this step's scope.
	"""
	system.db.runUpdateQuery("UPDATE partid_fms SET PartID=0,TimeStamp=NOW() WHERE machineID=1","FMSLine")
	shared.MES.trackEnd(chart.ID)
	if chart.ID!=0:
		system.tag.write("[Scada]ITE/iFMS/ProductionArea/FMSLine/IML/Status/Messages","No recored updated for tracking")</stop-script>
	</step>
	<step id="a186bf8a-b4ae-46ea-a153-976337ea9d30" location="5 17" name="ReadytoRun" factory-id="action-step">
		<start-script>def onStart(chart, step):
	"""
	This will run when the step starts, before any
	other action.

	Arguments:
		chart: A reference to the chart's scope.
		step: A reference to this step's scope.
	"""
	from time import sleep
	system.tag.write("[Scada]ITE/iFMS/ProductionArea/FMSLine/IML/Status/Messages","Initialize Done")
	system.tag.write("[Scada]ITE/iFMS/ProductionArea/FMSLine/IML/Command/Init_cmd_s",0)
	system.tag.write("[Scada]ITE/iFMS/ProductionArea/FMSLine/IML/Command/Stop_cmd_s",0)
	sleep(1)
	chart.value=system.tag.read("[Scada]ITE/iFMS/ProductionArea/FMSLine/IML/Status/Ready_Sts").value
	shared.cells.flagevents("ReadytoRun","IML",chart.value)
	system.tag.write("[Scada]ITE/iFMS/ProductionArea/FMSLine/IML/Status/Messages","Ready To Run")
#	sleep(1)
#	system.tag.write("ITE/iFMS/ProductionArea/FMSLine/LaserCut/w_Start",1)
	chart.partID=system.db.runScalarQuery("SELECT PartName FROM master_Parts WHERE processed=0 ORDER BY timestamp LIMIT 1","FMSLine")
	if chart.partID!=None and chart.partID!=0 and chart.partID!="0":
		system.tag.write("[Scada]ITE/iFMS/ProductionArea/FMSLine/IML/Command/PartID",chart.partID)
		system.tag.write("[Scada]ITE/iFMS/ProductionArea/FMSLine/IML/Command/Start_cmd_s",1)
		
	chart.idleTime=0</start-script>
		<stop-script>def onStop(chart, step):
	"""
	This will run when the step is finished and
	all other actions complete.

	Arguments:
		chart: A reference to the chart's scope.
		step: A reference to this step's scope.
	"""
	system.db.runPrepUpdate("UPDATE partid_fms SET PartID=?,TimeStamp=NOW() WHERE machineID=1",[chart.partID],"FMSLine")
	chart.ID=shared.MES.trackStart(chart.partID,"IML")
	if chart.ID==0:
		system.tag.write("[Scada]ITE/iFMS/ProductionArea/FMSLine/IML/Status/Messages","No recored inserted for tracking")
	else:
		chart.stepID=system.tag.read("[Scada]ITE/iFMS/ProductionArea/FMSLine/IML/StepID").value
		chart.trackID= shared.MES.startMachineOEE(chart.partID,"IML",chart.stepID)
		shared.MES.startLineOEE(chart.partID)</stop-script>
	<timer-script interval="1000">def onTimer(chart, step):
	"""
	This will run at a fixed interval while the
	step is active.

	Arguments:
		chart: A reference to the chart's scope.
		step: A reference to this step's scope.
	"""
	chart.partID=system.db.runScalarQuery("SELECT PartName FROM master_Parts WHERE processed=0 ORDER BY timestamp LIMIT 1","FMSLine")
	if chart.partID!=None and chart.partID!=0 and chart.partID!="0":
		system.tag.write("[Scada]ITE/iFMS/ProductionArea/FMSLine/IML/Command/PartID",chart.partID)
		system.tag.write("[Scada]ITE/iFMS/ProductionArea/FMSLine/IML/Command/Start_cmd_s",1)
		
	if chart.idleTime&gt;=system.tag.read("[Scada]ITE/iFMS/ProductionArea/FMSLine/IdleTimeParameter").value:
		system.tag.write("[Scada]ITE/iFMS/ProductionArea/FMSLine/IML/Production/MachineStatus",8)
	else:
		chart.idleTime=chart.idleTime+1</timer-script>
	</step>
	<step id="3c188496-b214-4ac7-8718-403d39827e0d" location="11 15" name="ReadytoUnload" factory-id="action-step">
		<start-script>def onStart(chart, step):
	"""
	This will run when the step starts, before any
	other action.

	Arguments:
		chart: A reference to the chart's scope.
		step: A reference to this step's scope.
	"""
	chart.value=system.tag.read("[Scada]ITE/iFMS/ProductionArea/FMSLine/IML/Status/ReadyToUnLoad_Sts").value
	shared.cells.flagevents("ReadytoUnload","IML",chart.value)
	shared.MES.endMachineOEE(chart.trackID,system.date.now())
	
	system.tag.write("[Scada]ITE/iFMS/ProductionArea/FMSLine/IML/Status/Messages","Ready To Unload")
	system.tag.write("[Scada]ITE/iFMS/ProductionArea/FMSLine/IML/Command/Start_cmd_s",0)
	
	chart.idleTime=0</start-script>
	<timer-script interval="1000">def onTimer(chart, step):
